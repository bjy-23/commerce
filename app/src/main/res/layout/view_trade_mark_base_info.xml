<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:orientation="vertical">
            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="商标名称"/>
            <TextView
                android:id="@+id/tvTradeName4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="猛狮"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="商标注册证号"/>
            <TextView
                android:id="@+id/tvRegisterNo4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="1015011"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="商标类别"/>
            <TextView
                android:id="@+id/tvType4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="32"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="是否共有商标"/>
            <TextView
                android:id="@+id/tvComTrade4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="否"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="是否地理标志商标"/>
            <TextView
                android:id="@+id/tvGeoGraphyTrade4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="是"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="是否知名著名商标"/>
            <TextView
                android:id="@+id/tvFamousTrade4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="是"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="初审公告期号"/>
            <TextView
                android:id="@+id/tvExameNotifyNo4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="580"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="注册公告期号"/>
            <TextView
                android:id="@+id/tvRegisterNotifyNo4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="592"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="申请日期"/>
            <TextView
                android:id="@+id/tvApplyDate4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="1995-02-09"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="指定颜色说明"/>
            <TextView
                android:id="@+id/tvColorSet4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="非颜色组合商标"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="放弃专用权说明"/>
            <TextView
                android:id="@+id/tvGiveUpAuthority4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="是"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:paddingLeft="5dp"
            android:orientation="vertical">
            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="商标名称意译"/>
            <TextView
                android:id="@+id/tvTransLateName4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="猛狮"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="商标类型"/>
            <TextView
                android:id="@+id/tvClassic4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="1015011"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="商标形式类型"/>
            <TextView
                android:id="@+id/tvFormType4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="32"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="是否立体商标"/>
            <TextView
                android:id="@+id/tv3DTrade4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="否"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="商标状态"/>
            <TextView
                android:id="@+id/tvTradestate4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="是"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="是否驰名商标"/>
            <TextView
                android:id="@+id/tvWellKnownTrade4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="是"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="初审公告日期"/>
            <TextView
                android:id="@+id/tvExameNotifyDate4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="580"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="注册公告日期"/>
            <TextView
                android:id="@+id/tvRegisterNotifyDate4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="592"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="专用权有效期"/>
            <TextView
                android:id="@+id/tvPrivateRightDate4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="1995-02-09"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="组合颜色数量"/>
            <TextView
                android:id="@+id/tvColorNum4TradeMarkDetail"
                style="@style/trade_mark_content_textview"
                android:text="非颜色组合商标"/>

        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            style="@style/trade_mark_tab_textview"
            android:layout_marginLeft="20dp"
            android:text="备注"/>
        <TextView
            style="@style/trade_mark_content_textview"
            android:id="@+id/tvComments4TradeMarkDetail"
            android:text="非颜色组合德尔福个"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            style="@style/trade_mark_tab_textview"
            android:layout_marginLeft="20dp"
            android:text="商标图片"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            android:src="@drawable/girl1"
            />
    </LinearLayout>

</LinearLayout>