<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:paddingLeft="20dp"
            android:orientation="vertical">
            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="是否名牌产品"/>
            <TextView
                android:id="@+id/tvIsFamousMark4TradeMarkOtherInfo"
                style="@style/trade_mark_content_textview"
                android:text="猛狮"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="是否地标产品"/>
            <TextView
                android:id="@+id/tvIsNewGoods4TradeMarkOtherInfo"
                style="@style/trade_mark_content_textview"
                android:text="1015011"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="是否高新技术企业"/>
            <TextView
                android:id="@+id/tvIsHighTech4TradeMarkOtherInfo"
                style="@style/trade_mark_content_textview"
                android:text="32"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="是否中华老字号"/>
            <TextView
                android:id="@+id/tvIsOldMark4TradeMarkOtherInfo"
                style="@style/trade_mark_content_textview"
                android:text="否"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="是否**省/市老字号"/>
            <TextView
                android:id="@+id/tvIsProvinceMark4TradeMarkOtherInfo"
                style="@style/trade_mark_content_textview"
                android:text="是"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="是否园区企业"/>
            <TextView
                android:id="@+id/tvIsZoneCompany4TradeMarkOtherInfo"
                style="@style/trade_mark_content_textview"
                android:text="是"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:paddingLeft="5dp"
            android:orientation="vertical">
            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="获得年份"/>
            <TextView
                android:id="@+id/tvFamousMarkDate4TradeMarkOtherInfo"
                style="@style/trade_mark_content_textview"
                android:text="猛狮"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="获得年份"/>
            <TextView
                android:id="@+id/tvGetLandMarkDate4TradeMarkOtherInfo"
                style="@style/trade_mark_content_textview"
                android:text="1015011"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="获得年份"/>
            <TextView
                android:id="@+id/tvGetHighTec4TradeMarkOtherInfo"
                style="@style/trade_mark_content_textview"
                android:text="32"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="获得年份"/>
            <TextView
                android:id="@+id/tvGetOldMarkDate4TradeMarkOtherInfo"
                style="@style/trade_mark_content_textview"
                android:text="否"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="获得年份"/>
            <TextView
                android:id="@+id/tvGetProvinceMarkDate4TradeMarkOtherInfo"
                style="@style/trade_mark_content_textview"
                android:text="是"/>

            <TextView
                style="@style/trade_mark_tab_textview"
                android:text="园区名称"/>
            <TextView
                android:id="@+id/tvZoneName4TradeMarkOtherInfo"
                style="@style/trade_mark_content_textview"
                android:text="是"/>

        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            style="@style/trade_mark_tab_textview"
            android:layout_marginLeft="20dp"
            android:text="备注"/>
        <TextView
            style="@style/trade_mark_content_textview"
            android:id="@+id/tvComments4TradeMarkOtherInfo"
            android:text=""/>
    </LinearLayout>


</LinearLayout>